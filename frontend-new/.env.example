# NASA API Configuration
# Get your free API key at: https://api.nasa.gov/
REACT_APP_NASA_API_KEY=DEMO_KEY

# Cesium Ion Access Token (optional - for enhanced terrain/imagery)
# Get your token at: https://cesium.com/ion/
REACT_APP_CESIUM_ION_TOKEN=your_cesium_token_here

# Backend API URL (if using separate backend)
REACT_APP_BACKEND_URL=http://localhost:5001

# Feature Flags
REACT_APP_ENABLE_TSUNAMI_SIMULATION=true
REACT_APP_ENABLE_ADVANCED_PHYSICS=true
REACT_APP_ENABLE_PDF_REPORTS=true
